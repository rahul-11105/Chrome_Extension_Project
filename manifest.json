{
    "manifest_version": 3,
    "name": "Tab Title Extension",
    "version": "1.0",
    "description": "Displays the title of the current tab in the popup.",
    "permissions": ["tabs"],
    "action": {
                "default_popup": "popup.html",
                "default_icon": "icons/icons8-title-48.png",
                "default_title": "Current Tab Title"
    },
    "icons": {
                "16": "icons/icons8-t-16.png",
                "48": "icons/icons8-title-48.png"
    }
  }
  